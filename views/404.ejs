

<% if (receipts.length >0) { 
    <% receipts.forEach(receipt => { %>
      <td><%= receipt.ID></td>
      <td><%= receipt.transaction_date %></td>
      if (totals.length >0) {
        <% totals.forEach(total => { %>
          if (total.id = receipt.id) {

        <td><%= total.total </td>
          }
        <% }) %>
        <% } %>
    <% }) %>
    <% } %>